{
    "version": 2,
    "builds": [
        { "src": "api.py", "use": "@vercel/python" }
    ],
    "routes": [
        { "src": "/", "dest": "/templates/index.html" },
        { "src": "/submit", "dest": "app.py" },
        { "src": "/static/(.*)", "dest": "app.py" },
        { "src": "/templates/(.*)", "dest": "app.py" }
    ]
}
